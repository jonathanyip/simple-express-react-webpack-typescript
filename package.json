{
  "name": "wilbo-pi-sup",
  "version": "2.0.0",
  "license": "UNLICENSED",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": "3",
          "targets": "> 0.01%, not dead"
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript",
      "@emotion/babel-preset-css-prop"
    ],
    "plugins": [
      "@emotion",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "dependencies": {
    "@types/express": "^4.17.11",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.0",
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/react": "^11.4.0",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^3.11.4",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.1.0",
    "eslint": "^7.26.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.10",
    "html-webpack-plugin": "^5.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "watch:api": "./node_modules/.bin/ts-node-dev --watch ./src/api/ ./src/api/app.ts",
    "watch:frontend": "./node_modules/.bin/webpack serve --env local --config webpack.config.ts",
    "watch": "./node_modules/.bin/concurrently 'yarn:watch:*'",
    "build:frontend": "./node_modules/.bin/webpack --env production --config webpack.config.ts",
    "build:api": "./node_modules/.bin/tsc --project tsconfig.api.json",
    "build": "./node_modules/.bin/concurrently 'yarn:build:*'",
    "docker:build": "docker-compose --file ./pipeline/docker-compose.yml build",
    "docker:up": "docker-compose --file ./pipeline/docker-compose.yml up"
  }
}
